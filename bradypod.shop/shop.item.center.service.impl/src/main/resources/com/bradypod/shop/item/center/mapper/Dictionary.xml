<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bradypod.shop.item.center.mapper.DictionaryMapper">

	<!-- 获取实体 -->
	<select id="get" parameterType="com.bradypod.shop.item.center.po.Dictionary" resultType="com.bradypod.shop.item.center.po.Dictionary">
		select * from
		t_dict_sys
		<where>
			1 = 1
		</where>
	</select>
	
	<!-- 获取全部实体 -->
	<select id="getAll" parameterType="com.bradypod.shop.item.center.po.Dictionary" resultType="com.bradypod.shop.item.center.po.Dictionary">
		<![CDATA[
		select * from t_dict_sys 
		]]>
	</select>
	
	<!-- 保存�?��实体 -->
	<insert id="save" parameterType="com.bradypod.shop.item.center.po.Dictionary"
		useGeneratedKeys="true" keyProperty="id">
		insert into t_dict_sys (task_id, task_desc, task_type, start_time, end_time, state, create_time, create_operator, last_modify, last_operator, job_id)
		values
		(
		${taskId}, '${taskDesc}', ${taskType}, '${startTime}', '${endTime}', ${state}, now(), '${createOperator}', now(), '${lastOperator}', ${jobId}
		)
	</insert>
	
	<!-- 更新�?��实体 -->
	<update id="update" parameterType="com.bradypod.shop.item.center.po.Dictionary">
		update t_dict_sys set
		
		<where>
		    1 = 1
		</where>
	</update>
	
	<!-- 删除�?��实体 -->
	<delete id="delete" parameterType="com.bradypod.shop.item.center.po.Dictionary">
		delete from t_dict_sys
		<where>
		    1 = 1
		</where>
	</delete>

</mapper>